import HeaderIntro from "../components/HeaderIntro.astro";
import TuinAanleg from "../components/TuinAanleg2.astro";
import TuinOnderhoud from "../components/TuinOnderhoud.astro";
import ManierVanWerken from "../components/ManierVanWerken.astro";
import Cta from "../components/Cta.astro";
import USP from "../components/USP_leftright.astro";
import USPWiggly from "../components/USP_wiggly.astro";
import Testimonials from "../components/Testimonials.astro";
import Grondwerken from "../components/Grondwerken.astro";
import ContactForm from "../components/ContactForm.astro";
